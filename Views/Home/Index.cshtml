@using Microsoft.AspNetCore.Html
@model IEnumerable<ToDoModel>
@{
    ViewData["Title"] = "Home Page";
}

<!--Render to do Items -->


<h2>To-Do List</h2>
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <partial name="PartialViews/ToDoListControl" model="Model" />
            <partial name="PartialViews/AddToDoItemControl", model="@new ToDoModel{}" />
        </div>
    </div>
</div>

<script src="~/js/UpdatableToDoScript.js"></script>